{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2018",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "removeComments": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "references": [
    {
      "path": "../app1"
    }
  ],
  "include": [
    "src",
    "../app1/src"
  ],
  "exclude": [
    "build",
    "scripts",
    "node_modules",
    "ios",
    "android"
  ]
}